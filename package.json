  "devDependencies": {
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.6",
    "jasmine-spec-reporter": "^7.0.0",
    "protractor": "^7.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "eslint": "^8.1.0",
